public class Book {


String _name;
String _author;
int _сору;
public String get_name () {
return _name;
}
public void set_name(String _name) {
this. _name
= _name;
}
public String get_author() {
return _author;
}
public void set_author (String _author) {
this._author = _author;
}
public int get_сору() {
return _сору;
}
public void set_copy(int _copy) {
	this._сору = _copy;
}
}
